journal = {Journal of Physical Chemistry},
keywords = {Pchem Data},
mendeley-tags = {Pchem Data},
number = {2},
pages = {268--277},
publisher = {American Chemical Society},
title = {{Thermodynamics of electrolytes. I. Theoretical basis and general equations}},
volume = {77},
year = {1973}
}

